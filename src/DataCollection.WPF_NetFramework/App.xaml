<Application x:Class="Esri.ArcGISRuntime.OpenSourceApps.DataCollection.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Startup="Application_Startup"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="SharedConverters.xaml" />
                <ResourceDictionary Source="Themes/Light.xaml" />
                <ResourceDictionary Source="Styles/Brushes.xaml" />
                <ResourceDictionary Source="Icons.xaml" />
                <ResourceDictionary Source="Styles/SizesAndAlignment.xaml" />
                <ResourceDictionary Source="Styles/Controls.xaml" />
                <ResourceDictionary Source="Styles/ControlOverrides.xaml" />
                <ResourceDictionary Source="Styles/DataTemplates.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
